{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "object-literal-sort-keys": false,
        "interface-name": false,
        "no-shadowed-variable": false,
        "ordered-imports": false,
        "callable-types": false,
        "no-reference-import": false,
        "no-var-requires": false,
        "ban-types": false,
        "quotemark": [
            true,
            "single"
        ],
        "no-console":false
    },
    "rulesDirectory": []
}
